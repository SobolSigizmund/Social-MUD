<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<!-- Zone 30 -->
	<bean id="createMonsterFidoAction"	class="com.smud.model.action.CreateMonsterResetAction">
		<property name="monsterDefinition" ref="monster3062"/>
		<property name="maxQuantity" value="2"/>
	</bean>
	
	<bean id="createItemFountainAction"	class="com.smud.model.action.CreateItemInRoomResetAction">
		<property name="itemDefinition" ref="item3035"/>
		<property name="maxQuantity" value="1"/>
	</bean>
	
	<bean id="createItemAxeAction"	class="com.smud.model.action.CreateItemInRoomResetAction">
		<property name="itemDefinition" ref="item3024"/>
		<property name="maxQuantity" value="1"/>
	</bean>
	
	<bean id="createItemTorchAction" class="com.smud.model.action.CreateItemInRoomResetAction">
		<property name="itemDefinition" ref="item3030"/>
		<property name="maxQuantity" value="1"/>
	</bean>
	
	<bean id="createItemShieldAction" class="com.smud.model.action.CreateItemInRoomResetAction">
		<property name="itemDefinition" ref="item3042"/>
		<property name="maxQuantity" value="1"/>
	</bean>
</beans>